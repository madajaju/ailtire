@startuml


boundary __ts_family #blue


entity __family #orange


package  "Top Stuff"  lightblue {
    card children #blue [
        <b><color:white>children</color></b>
    ]
    
    __ts_family -[#blue]- children
    
    
    card sibling #red [
        <b><color:white>sibling</color></b>
    ]
    
    
    card parent #orange [
        <b><color:white>parent</color></b>
    ]
    
    
    __family -[#orange]- parent
    
    artifact images #ffffff {
        
        file "_ts_web"
        
        file "_ts_gw"
        
    }

    node frontend
    frontend .. _ts_web
    
        sibling -[#red]- frontend
    

    node gw
    gw .. _ts_gw
    
        children -[#blue]- gw
    
        sibling -[#red]- gw
    




    
}




package exampleusecase #lightgray {
    card exampleusecasechildren #orange [
        <b><color:white>children</color></b>
    ]
    exampleusecasechildren ==[#orange]= parent
}


@enduml
