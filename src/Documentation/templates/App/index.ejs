<html style="overflow:hidden;">

<head>
    <link rel="stylesheet/less" type="text/css" href="/styles/importer.less"/>
    <script src="/js/less.js" type="text/javascript"></script>
</head>
<div class="leftContent">
    <h3>Top</h3>
    <ul class="left-actionbar">
        <li><a href='/<%= appName %>/index.html' target="mainFrame"><%= appName %></a></li>
    </ul>
    <h3>Actors</h3>
    <ul class="left-actionbar">
        <% for(let aname in actors) { %>
            <li><a href='/doc/actor/<%= actors[aname].shortname %>' target="mainFrame"><%= aname %></a></li>
        <% } %>
    </ul>
    <h3>Packages</h3>
    <ul class="left-actionbar treeview">
        <%- partial("./templates/App/_packageTree.ejs", {pkgs: packages}) %>
    </ul>
    <h3>Use Cases</h3>
    <ul class="left-actionbar">
        <% for(let pname in packages) {
                for(let uname in packages[pname].usecases) {
            let usecase = packages[pname].usecases[uname];
        %>
        <li><a href='/doc/usecase<%= usecase.prefix %>/<%= uname %>' target="mainFrame"><%= usecase.name %></a></li>
        <% }
        } %>
    </ul>
</div>

<div class="rightContent">
    <iframe name="mainFrame" style="border:none;" src="./<%= appName %>/index.html" width="100%" height="100%"></iframe>
</div>
</html>
