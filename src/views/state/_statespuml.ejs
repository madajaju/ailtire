
<%
let names = name.split(/\//);
let asName = '';
names.shift(); // Shift off the first item that is blank.
for(let i in names) {
    let aname = names[i];
    if(global.packages.hasOwnProperty(aname)) {
        color = global.packages[aname].definition.color;
    }
    asName += aname;
    %>
state "<%= aname %>" as <%= asName %> <% color %> {
<% } %>
<%= '}\n'.repeat(anames.length); %>
