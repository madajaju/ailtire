@startuml
  interface CLI
  interface Web
  interface WebSocket
package "exampleStacks" #lightgreen {
  component express as REST
  component socket.io as socket
  folder bin #ffffff {
    component "examplestacks" as binName
  }
  binName --> REST

  folder deploy #ffffff {
  
    file "web" as web_container
    REST --> web_container
    socket ---> web_container
  
    file "doc" as doc_container
    REST --> doc_container
    socket ---> doc_container
  
  }
  folder images #ffffff {
  
      artifact examplestacks_web
      web_container --> examplestacks_web
    
      artifact examplestacks_doc
      doc_container --> examplestacks_doc
    
  }
}
CLI -- binName
Web - REST
WebSocket -- socket

@enduml
